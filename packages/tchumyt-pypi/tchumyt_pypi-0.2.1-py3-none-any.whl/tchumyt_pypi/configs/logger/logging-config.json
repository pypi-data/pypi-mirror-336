{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "standard": {
            "format": "%(asctime)s - %(module)s - %(levelname)s - %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "standard",
            "level": "DEBUG"
        }
    },
    "loggers": {
        "tchumyt": {
            "handlers": ["console"],
            "level": "INFO",
            "propagate": "True"
        }
    }
}