{
  "name": "dash_blueprint_components",
  "version": "0.3.0",
  "description": "Dash plotly porting of BlueprintJS",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:backends": "dash-generate-components ./src/lib/components dash_blueprint_components -p package-info.json --r-prefix '' --jl-prefix '' --ignore \\.test\\.",
    "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:backends)",
    "build": "npm run build:js && npm run build:backends",
    "build:activated": "npm run build:js && npm run build:backends-activated"
  },
  "author": "Stefano Fanizzi <ste.fanizzi@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "^5.17.6",
    "@blueprintjs/datetime2": "^2.3.30",
    "@blueprintjs/docs-theme": "^5.3.30",
    "@blueprintjs/icons": "^5.20.0",
    "@blueprintjs/select": "^5.3.18",
    "dateutil": "^0.1.0",
    "ramda": "^0.26.1",
    "react": "^18.3.1",
    "react-day-picker": "^9.6.3",
    "react-dom": "^18.3.1",
    "styled-jsx": "^5.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@plotly/dash-component-plugins": "^1.2.3",
    "@plotly/webpack-dash-dynamic-import": "^1.3.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.2.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.11.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "prop-types": "^15.8.1",
    "react-docgen": "^5.4.3",
    "style-loader": "^3.3.4",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
