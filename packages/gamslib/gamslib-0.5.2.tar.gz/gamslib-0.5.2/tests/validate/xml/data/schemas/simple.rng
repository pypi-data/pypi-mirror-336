<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0"
         datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <start>
    <element name="products">
      <zeroOrMore>
        <element name="product">
          <interleave>
            <element name="name">
              <text/>
            </element>
            <element name="price">
              <text/>
            </element>
          </interleave>
        </element>
      </zeroOrMore>
    </element>
  </start>
</grammar>
