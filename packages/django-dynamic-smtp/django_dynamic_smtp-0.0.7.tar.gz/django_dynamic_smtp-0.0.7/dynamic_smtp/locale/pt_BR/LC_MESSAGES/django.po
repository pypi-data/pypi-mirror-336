msgid "Use SSL"
msgstr "Usar SSL"

#: models.py:52
msgid "Timeout"
msgstr "Timeout"

#: models.py:56
msgid "Sender name"
msgstr "Nome do remetente"

#: models.py:61
msgid "Sender email"
msgstr "Email do remetente"

#: models.py:67
msgid "Email signature"
msgstr "Assinatura dos emails"

#: models.py:70
msgid "To use this signature in email, use the variable"
msgstr "Para usar essa assinatura nos emails, use a variável"
