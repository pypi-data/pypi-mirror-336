age for the corresponding Java package https://peps.python.org/pep-0561/#stub-only-packages\n    \"org-stubs\" # stub-only package for the corresponding Java package https://peps.python.org/pep-0561/#stub-only-packages\n]\n\n[tool.uv]\ndev-dependencies = [\n    \"pytest\",\n    \"coverage\",\n    \"pytest-cov\"\n]\n\n[tool.uv.workspace]\nmembers = [\"examples\"]\n\n# Use local orekit-jpype for this workspace\n[tool.uv.sources]\norekit-jpype = { workspace = true }\n
